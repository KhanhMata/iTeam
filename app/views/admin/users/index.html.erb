<% provide :title, t("allusers") %>
<h1><%= t "allusers" %></h1>

<ul class="users">
  <% @users.each do |user| %>
  <li>
  <%= link_to user.name, user %>
  <% if !is_user?(user) %>
    <%= link_to "delete", admin_user_path(user), method: :delete,
      data: {confirm: t("you_sure")} %>
  <% end %>
</li>

  <% end %>
</ul>

<%= will_paginate @users %>
